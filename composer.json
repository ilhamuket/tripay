{
    "name": "ilhamuket/tripay",
    "description": "Tripay Payment Gateway SDK for Laravel 10, 11, 12 and PHP 8.2+",
    "keywords": [
        "tripay",
        "payment",
        "payment-gateway",
        "qris",
        "virtual-account",
        "e-wallet",
        "laravel",
        "indonesia"
    ],
    "homepage": "https://github.com/ilhamuket/tripay",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ilham Uket",
            "email": "ilhamgallagher633@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ilhamuket\\Tripay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ilhamuket\\Tripay\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ilhamuket\\Tripay\\TripayServiceProvider"
            ],
            "aliases": {
                "Tripay": "Ilhamuket\\Tripay\\Facades\\Tripay"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
